pico-8 cartridge // http://www.pico-8.com
version 29
__lua__
t=0
wind=1
wvel=0
windy=true

function _update()
	if btnp(4) then
		windy=not windy
	end
	local target = windy and 1 or 0.2
	wvel+=0.01*(target-wind)
	wind+=wvel
	wvel/=1.1
end

function _draw()
	cls()
	line(64, 64, 64, 64+16, 4)
	for x=16,0,-1 do
		local off = x~=0 and sin((x+t)/16)*wind or 0
		local ang = 1-(wind/4)
		local xoff = sin(ang)*x
		local yoff = cos(ang)*x
		tline(64+xoff,64+off+yoff,64+xoff,64+8+off+yoff,x/8,0,0,1/8)
	end
	t+=1
end

function appr(val,target,amount)
  return val>target and max(val-amount,target) or min(val+amount,target)
end
